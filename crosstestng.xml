<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
	<listener class-name="extendReport.ExtentTestNGIReporterListener" />
</listeners>
  <test thread-count="5" name="chromeTest">
    <parameter name="browsers" value="Chrome"></parameter>
    <classes>
      <class name="crossbrowserTESTNG.Verifyhoodie"/>
      <class name="crossbrowserTESTNG.Verifypant"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="firefoxTest">
    <parameter name="browsers" value="firefox"></parameter>
    <classes>
      <class name="crossbrowserTESTNG.Verifyhoodie"/>
      <class name="crossbrowserTESTNG.Verifypant"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="edgeTest">
    <parameter name="browsers" value="edge"></parameter>
    <classes>
      <class name="crossbrowserTESTNG.Verifyhoodie"/>
      <class name="crossbrowserTESTNG.Verifypant"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
